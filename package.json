{
  "name": "zootools_challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "npm run type-css",
    "lint": "next lint",
    "format": "prettier --write --ignore-unknown .",
    "format:check": "prettier --check --ignore-unknown .",
    "type:check": "tsc --noEmit",
    "type-css": "npx tcm src",
    "type-css:watch": "npx tcm src --watch",
    "type-css:check": "npx tcm src --listDifferent",
    "all:check": "npm run type:check && npm run type-css:check && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@types/node": "20.5.4",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "eslint": "^8.47.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "prettier": "^3.0.2",
    "typed-css-modules": "^0.7.2"
  }
}
